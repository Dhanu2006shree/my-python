def is_armstrong():
  num = int(input("enter a number: "))
  num_str = str(num)  
def is_armstrong():
  num = int(input("enter a number: "))
  num_str = str(num)  
  num_digits = len(num_str)  
  sum_of_powers = 0
  for digit in num_str:
    sum_of_powers += int(digit) ** num_digits
  if sum_of_powers == num:
    return True  
  else:
    return False  
result = is_armstrong() 
print(result)
result = is_armstrong() 
print(result)
